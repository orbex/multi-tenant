    "keywords": ["laravel", "multi-tenant", "multi-tenancy", "hyn"],
    "license": "MIT",
    "authors": [
        {
            "name": "DaniÃ«l `Luceos` Klabbers",
            "email": "daniel+hyn@klabbers.email"
        }
    ],
    "support": {
        "issues": "https://github.com/orbex/multi-tenant/issues",
        "source": "https://github.com/orbex/multi-tenant",
        "docs": "https://hyn.readme.io"
    },
    "require": {
        "php": ">=5.5.9",
        "orbex/framework": "*",
        "orbex/webserver": "*"
    },
    "suggest": {
